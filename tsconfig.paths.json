{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      // =============================== [global]
      "@__tests__/*": [
        "__tests__/*"
      ],
      // =============================== [src]
      "@config/*": [
        "src/config/*"
      ],
      "@libs/*": [
        "src/libs/*"
      ],
      "@middleware/*": [
        "src/middleware/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@prismaCall/*": [
        "src/prisma/*"
      ],
      // =============================== [src / v1]
      "@v1/*": [
        "src/v1/*"
      ],
      "@v1Controllers/*": [
        "src/v1/controllers/*"
      ],
      "@v1Graphql/*": [
        "src/v1/graphql/*"
      ],
      "@v1GraphqlNonAuthSchema/*": [
        "src/v1/graphqlNonAuthSchema/*"
      ],
      "@v1GraphqlAdminSchema/*": [
        "src/v1/graphqlAdminSchema/*"
      ],
      "@v1GraphqlUserSchema/*": [
        "src/v1/graphqlUserSchema/*"
      ],
      "@v1Routes/*": [
        "src/v1/routes/*"
      ],
      "@v1Utils/*": [
        "src/v1/utils/*"
      ],
    }
  }
}